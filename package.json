{
  "name": "@bredansky/styleguide",
  "version": "0.0.14",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/styleguide.js",
      "types": "./dist/styleguide.d.ts"
    },
    "./typescript": "./typescript/base.json"
  },
  "files": [
    "dist",
    "typescript",
    "types"
  ],
  "scripts": {
    "build": "tsup",
    "check": "npm run fix && npm run typecheck",
    "fix": "eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "!(*.ts|*.mts|*.cts|*.tsx|*.js|*.cjs|*.mjs|*.jsx)": "prettier --write --ignore-unknown",
    "*.ts|*.mts|*.cts|*.tsx|*.js|*.cjs|*.mjs|*.jsx": "eslint --fix"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint-react/eslint-plugin": "^1.53.0",
    "@eslint/js": "^9.27.0",
    "@next/eslint-plugin-next": "^15.3.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.15.29",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0",
    "prettier-plugin-packagejson": "^2.5.14",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript-eslint": "^8.33.1"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=22.16"
  }
}
